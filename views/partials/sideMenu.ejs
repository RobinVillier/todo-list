<div id="sideMenu" class="open">
    <div class="sortingButton">
        <img src="/images/task_icon.svg" alt="Task Icon">
        <p>All Lists</p>
    </div>
    <div class="sortingButton">
        <img src="/images/fav_icon.svg" alt="Fav Icon">
        <p>Favorites</p>
    </div>
    <div class="divider"></div>
    <div id="listsTitle">
        <p>Lists</p>
        <img src="/images/plus_icon.svg" alt="Plus Icon" id="plusIcon">
    </div>
    <ul>
        <% lists.forEach(list => { %>
            <% if (list.active) { %>
                <li class="listItem active" data-name="<%= list.name %>">
                    <p><%= list.name %></p>
                </li>
            <% } else { %>
                <li class="listItem" data-name="<%= list.name %>">
                    <p><%= list.name %></p>
                </li>
            <% } %>
        <% }) %>
    </ul>
</div>